{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "arrow-parens": false,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "trailing-comma": [
      false
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "max-line-length": [
      true,
      140
    ],
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ]
  },
  "rulesDirectory": []
}
